{
  "name": "vue-cli-plugin-html-validate",
  "version": "4.0.0",
  "description": "html-validate plugin for vue-cli",
  "keywords": [
    "vue",
    "cli",
    "html-validate"
  ],
  "homepage": "https://html-validate.org",
  "bugs": {
    "url": "https://gitlab.com/html-validate/vue-cli-plugin-html-validate/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/html-validate/vue-cli-plugin-html-validate.git"
  },
  "license": "MIT",
  "author": "David Sveningsson <ext@sidvind.com>",
  "main": "index.js",
  "files": [
    "*.js",
    "generator",
    "!.eslintrc.js"
  ],
  "scripts": {
    "eslint": "eslint --cache .",
    "eslint:fix": "eslint --cache --fix .",
    "prepare": "husky install",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "false"
  },
  "commitlint": {
    "extends": "@html-validate"
  },
  "lint-staged": {
    "*.{ts,js,json,md,scss}": "prettier --write"
  },
  "prettier": "@html-validate/prettier-config",
  "dependencies": {
    "@vue/cli-shared-utils": "^5.0.0"
  },
  "devDependencies": {
    "@html-validate/commitlint-config": "3.0.16",
    "@html-validate/eslint-config": "5.9.8",
    "@html-validate/prettier-config": "2.3.10",
    "html-validate": "8.0.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "npm-pkg-lint": "1.14.1",
    "prettier": "2.8.8"
  },
  "peerDependencies": {
    "html-validate": "^5 || ^6 || ^7 || ^8"
  },
  "engines": {
    "node": ">= 16.0"
  },
  "release": {
    "extends": "@html-validate/semantic-release-config"
  },
  "renovate": {
    "extends": [
      "gitlab>html-validate/renovate-config"
    ]
  }
}
